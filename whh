--//////////////////////////////////////////////////
--// SAFETY DEFAULTS (PREVENT MENU CRASH)
--//////////////////////////////////////////////////

WallHack.Settings = WallHack.Settings or {Enabled = false, TeamCheck = false, AliveCheck = true}
WallHack.Visuals = WallHack.Visuals or {}

local function def(tbl, defaults)
	for k,v in pairs(defaults) do
		if tbl[k] == nil then
			tbl[k] = v
		end
	end
end

def(WallHack.Visuals, {})
def(WallHack.Visuals.ESPSettings or {}, {
	Enabled = false,
	DisplayName = true,
	DisplayDistance = true,
	DisplayHealth = true,
	TextSize = 13,
	TextTransparency = 0,
	Offset = 0
})

def(WallHack.Visuals.BoxSettings or {}, {
	Enabled = false,
	Type = 1,
	Thickness = 1,
	Transparency = 0
})

def(WallHack.Visuals.ChamsSettings or {}, {
	Enabled = false,
	Filled = true,
	Transparency = 0.5
})

